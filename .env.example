# Telegram Bot Token (получите от @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Remnawave Panel Configuration
REMNAWAVE_BASE_URL=https://your-panel.domain.com
REMNAWAVE_TOKEN=your_remnawave_api_token_here
REMNAWAVE_CADDY_TOKEN=your_caddy_token_here

# User Creation Settings
# Inbound IDs могут быть числами или UUID (разделенные запятыми)
# Примеры: DEFAULT_INBOUND_IDS=1,2,3 или DEFAULT_INBOUND_IDS=b9811fcd-f20b-45c2-912a-fb21ab6c7664,another-uuid
DEFAULT_INBOUND_IDS=1,2,3
DEFAULT_PROTOCOL=vless
DEFAULT_UUID_PREFIX=promo-
SUBSCRIPTION_FILE_BASE_URL=https://your-file-server.com/files/

# Limits and Settings
MAX_SUBSCRIPTIONS_PER_REQUEST=100

# Admin User IDs (числовые ID пользователей Telegram, разделенные запятыми)
# Получите ваш ID через @userinfobot
ADMIN_USER_IDS=123456789,987654321

# Logging
LOG_LEVEL=INFO